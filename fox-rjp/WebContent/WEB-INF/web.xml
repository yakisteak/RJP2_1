<web-app xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">
	<servlet>
		<servlet-name>foxservlet</servlet-name>
		<servlet-class>FoxServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>foxservlet</servlet-name>
		<url-pattern>/foxServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>foxresult</servlet-name>
		<jsp-file>/foxresult.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>foxresult</servlet-name>
		<url-pattern>/foxresult</url-pattern>
	</servlet-mapping>
</web-app>